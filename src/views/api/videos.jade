section.sidebar.show
	.search
		.col-lg-8
			.input-group
				span.input-group-addon
					i.glyphicon.glyphicon-search
				input.searchbox.form-control(type="text", placeholder="Search", style="height:39px", data-type="video")
				.layer
	if (data.message !== null)
		h1.message
			i.fa.fa-circle-o-notch.fa-spin 
			span.text= data.message
	.user-sidebar.library-view(style="right: 22%;")
		.library
			ul.lib.group.artist.open

script.
	function initView(){
		$.ongaku.library.bind();
		$.ongaku.library.reset();
		$.ongaku.library.setPage(0);
		$.ongaku.library.clear();
		$.ongaku.library.loadVideo();
		$.ongaku.library.fetch();
	}
